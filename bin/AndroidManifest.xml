<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.classes.altitude"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon2"
        android:label="@string/app_name"
        android:theme="@style/CustomActionBarTheme" 
        >
        <activity
            android:name="com.classes.altitude.MainActivity"
            android:label="@string/app_name"
            android:configChanges="screenSize|orientation|keyboardHidden" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.classes.altitude.ResultsPage"
            android:label="Results" >
        </activity>
        <activity
            android:name="com.classes.altitude.Display"
            android:label="Results" >
        </activity>
        <activity
            android:name="com.classes.altitude.DetailView"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.classes.altitude.CreditsPage"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>